{
    "version": 2,
    "build": {
        "command": "npm run build",
        "output": "dist"
    },
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.html"
        }
    ]
}